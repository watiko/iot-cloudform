{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
      "member-access": [true, "no-public"],
      "interface-name": [true, "never-prefix"],
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore"
      ],
      "no-empty": false,
      "object-literal-sort-keys": false
    },
    "linterOptions": {
      "exclude": ["node_modules/**/*.ts"]
    }
  }